{
  "exclude": [
    "angular-nini/",
    ".vscode/",
    "node_modules/",
    "dist/",
    "node_modules",
    "./node_modules",
    "./node_modules/*",
    "./node_modules/@types/node/index.d.ts",
  ],
    "compilerOptions": {
      "module": "commonjs",
      "esModuleInterop": true,
      "skipLibCheck": true,
      "target": "es6",
      "moduleResolution": "node",
      "sourceMap": true,
      "outDir": "dist"
    },
    "lib": ["es2015"]
  }
  